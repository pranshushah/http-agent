{
  "version": "0.6.1",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "default": "./dist/index.d.ts"
      },
      "default": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      },
      "browser": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "description": "lightweight fetch based http client for browser",
  "bugs": {
    "email": "pranshu.shah23@gmail.com",
    "url": "https://github.com/pranshushah/http-pro/issues"
  },
  "keywords": [
    "fetch",
    "request",
    "response",
    "http",
    "https",
    "axios",
    "ky",
    "got",
    "node-fetch",
    "URL",
    "network",
    "rest",
    "ajax",
    "xmlhttprequest",
    "api",
    "browser"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "lint": "tsc",
    "prepare": "husky install && yarn build",
    "size": "size-limit",
    "analyze": "size-limit",
    "build": "tsup",
    "test": "npx jest"
  },
  "peerDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/pranshushah/http-pro"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "http-pro",
  "author": "pranshu shah",
  "type": "module",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "6 KB"
    }
  ],
  "devDependencies": {
    "@http-pro/validator": "^0.1.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^29.5.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "size-limit": "^7.0.8",
    "ts-jest": "^29.1.1",
    "tslib": "^2.4.0",
    "tsup": "^7.2.0",
    "typescript": "^4.3.4",
    "yup": "^1.3.2",
    "zod": "^3.22.4"
  },
  "dependencies": {}
}
